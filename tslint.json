{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "semicolon": [true, "never"],
      "quotemark": [true, "single", "backtick"],
      "no-console": [false],
      "variable-name": [
        true,
        "allow-leading-underscore"
      ]
  },
  "rulesDirectory": []
}