{
  "name": "humbahumbabot",
  "version": "1.0.0",
  "description": "킹갓제너럴뎀바바의 충견 스티븐 훔바훔바가 긁어주는 해외축구 뉴스",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all tsc",
    "dev:build": "npm-run-all lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node -r dotenv/config .",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "coverage": "nyc -r text -e .ts -x \"spec/*.spec.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belldoor/humbahumbabot.git"
  },
  "author": "belldoor <belldoor444@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/belldoor/humbahumbabot/issues"
  },
  "homepage": "https://github.com/belldoor/humbahumbabot#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.22",
    "@types/winston": "^2.4.4",
    "body-parser": "^1.19.0",
    "chrome-aws-lambda": "^2.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.29.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "telegraf": "^3.34.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.6.2",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.1.0",
    "puppeteer": "^2.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  }
}
